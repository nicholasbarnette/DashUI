{
	"name": "@nickbarnette/dashui",
	"version": "0.4.1",
	"description": "A lightweight React UI/component library.",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"start": "start-storybook -p 9000 -c .storybook",
		"build": "rm -rf build/ && webpack --config webpack.config.js --progress",
		"test": "jest",
		"format": "npm run format:prettier && npm run format:tslint",
		"format:prettier": "prettier --write ./src",
		"format:tslint": "tslint -p tsconfig.json -c tslint.json ./src/**/*.{ts,tsx,js,jsx} --fix",
		"prepublish": "npm run test && npm run build"
	},
	"lint-staged": {
		".src/**/*.{ts,tsx,js,jsx}": [
			"tslint -p tsconfig.json -c tslint.json --fix",
			"prettier --write"
		],
		".src/**/*.scss": [
			"prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [],
	"author": "Nick Barnette",
	"license": "UNLICENSED",
	"dependencies": {
		"classnames": "^2.2.6"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/preset-env": "^7.10.2",
		"@babel/preset-react": "^7.10.1",
		"@babel/preset-typescript": "^7.10.1",
		"@storybook/addon-contexts": "^5.3.19",
		"@storybook/react": "^5.3.19",
		"@testing-library/react": "^10.2.1",
		"@testing-library/react-hooks": "^3.4.2",
		"@types/chai": "^4.2.11",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^26.0.0",
		"@types/mocha": "^7.0.2",
		"@types/react": "^16.9.36",
		"@types/react-dom": "^16.9.8",
		"babel-loader": "^8.1.0",
		"chai": "^4.2.0",
		"copy-webpack-plugin": "^6.2.0",
		"core-js": "^3.6.5",
		"css-loader": "^3.6.0",
		"husky": "^4.2.5",
		"jest": "^26.0.1",
		"jest-fetch-mock": "^3.0.3",
		"lint-staged": "^10.2.11",
		"mini-css-extract-plugin": "^0.9.0",
		"mocha": "^8.0.1",
		"node-sass": "^4.14.1",
		"prettier": "^2.0.5",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-test-renderer": "^16.13.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.2.1",
		"ts-jest": "^26.1.0",
		"ts-loader": "^7.0.5",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.2",
		"tslint-immutable": "^6.0.1",
		"typescript": "^3.9.5",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"d3": "^5.16.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nicholasbarnette/DashUI.git"
	},
	"bugs": {
		"url": "https://github.com/nicholasbarnette/DashUI/issues"
	},
	"homepage": "https://github.com/nicholasbarnette/DashUI#readme"
}
